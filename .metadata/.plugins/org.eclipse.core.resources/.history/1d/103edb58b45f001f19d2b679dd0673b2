package _240821;

import java.util.Scanner;

public class 소수_2581 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		
			
			
			int M = sc.nextInt();
			int N = sc.nextInt();	
			int sum = 0;
			int min = 0;
			
		for(int i = M; i<Math.sqrt(N); i++) {
			if(i%i!= 0 )
			{
				sum += i;
			}
		}
		sc.close();
		System.out.println(sum);
		
	
	}

}
