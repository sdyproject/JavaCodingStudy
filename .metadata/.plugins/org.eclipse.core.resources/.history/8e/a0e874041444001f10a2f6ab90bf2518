package _240717;

import java.util.Scanner;

public class A플러스B_5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int total = 0;
		
		
		while(true) {
			int A = sc.nextInt();
			int B = sc.nextInt();
			total += A+B;
			
			if (total == 0) {
			 sc.close();
			 break;
			}
			System.out.println(total);
		}
		
		
		
	}

}
