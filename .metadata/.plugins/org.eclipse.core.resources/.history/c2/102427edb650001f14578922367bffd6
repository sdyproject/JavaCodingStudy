package _240802;

import java.util.Scanner;

public class 평균_1546 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
//		int N[] =new int[2];
//		
//		for(int i =0; i<3; i++) {
//			
//		}
		int N = sc.nextInt();
		int A[] = new int[N];
		int total = 0;
		
		for(int i =0; i<N; i++ ) {
			A[i] = sc.nextInt();
			total+= A[i];
		}
		System.out.println(total/N);
		
		
	}

}
