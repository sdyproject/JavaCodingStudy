package _240802;

import java.util.Arrays;
import java.util.Scanner;

public class 평균_1546 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		/*일반적인 평균 구하기 
		int N = sc.nextInt();
		int A[] = new int[N];
		int total = 0;
		
		for(int i =0; i<N; i++ ) {
			A[i] = sc.nextInt();
			total+= A[i];
		}
		System.out.println(total/N);*/
		
		int N = sc.nextInt();
		
		int A[] = new int[N];
		int total = 0;
		for(int i =0; i<N; i++ ) {
			A[i] = sc.nextInt();
		}
		Arrays.sort(A);
		
		for(int i =0; i<N; i++) {
			
			total += A[i]; 
		}
		System.out.println(total+"total");
		System.out.println(total*100.0/A[N-1]);
		
	}

}
