package _240802;

import java.util.Arrays;
import java.util.Scanner;

public class 최댓값_2562 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int A[] = new int[0];
		for(int i=0; i<10;  i++) {
			int B = sc.nextInt();
			A[i] = B;
		}
		Arrays.sort(A);
		System.out.println(A[10-1]);
	}

}
