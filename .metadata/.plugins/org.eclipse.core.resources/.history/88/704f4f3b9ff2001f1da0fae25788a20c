package _250224;

import java.util.Scanner;

public class 덧셈식출력 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		 int a = sc.nextInt();
	      int b = sc.nextInt();
	      sc.close();
	       System.out.print("a + b ="+ a+b);
	}
}
