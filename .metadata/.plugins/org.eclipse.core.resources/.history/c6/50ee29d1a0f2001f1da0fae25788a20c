package _250224;

import java.util.Scanner;

public class 문자열돌리기 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
        String a = sc.next();
        char b = 0;
        for(int i =0; i<a.length(); i++){
             b = a.charAt(i);
             System.out.println(b);
        }
        
	}

}
