package _240717;

import java.util.Scanner;

public class 숫자의합 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int N = sc.nextInt();
		int total = 0;
		
		for(int i =0; i<N; i++) {
			int A = sc.nextInt();
			
			total += A;
		}
		System.out.println(total);
	}

}
