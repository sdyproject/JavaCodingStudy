package _240829;

import java.util.Arrays;
import java.util.Scanner;

public class 소트인사이드 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in); 
		
		String N =  sc.next();
		String[] A = new String[N.length()];
		for(int i = 0; i<N.length(); i++) {
			A[i] = N.charAt(i);
		}
	}

}
